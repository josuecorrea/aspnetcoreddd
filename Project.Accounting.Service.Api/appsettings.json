{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "CacheConnection": "localhost:6379",
    "BrokerConnection": "rabbitmq://localhost:5672"
  },
  "AllowedHosts": "*",
  "Keycloak": {
    "realm": "accounting",
    "auth-server-url": "http://localhost:8080/",
    "ssl-required": "none",
    "resource": "accounting-client",
    "public-client": true,
    "verify-token-audience": true,
    "confidential-port": 0
  }
}